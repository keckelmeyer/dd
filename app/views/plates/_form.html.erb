<%= simple_form_for(@plate) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :state %>
    <%= f.input :plate %>
    <%= f.input :turning %>
    <%= f.input :turning %>
    <%= f.input :speed %>
    <%= f.input :speed %>
    <%= f.input :merging %>
    <%= f.input :merging %>
    <%= f.input :parking %>
    <%= f.input :parking %>
    <%= f.input :general %>
    <%= f.input :general %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
